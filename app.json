{
  "expo": {
    "name": "reactNativeMap",
    "slug": "reactNativeMap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.badyanchik.reactnative.map",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBBJomEKGW9nTi-55OX4z8esEJYGOwf-PM"
        }
      }
    }
  }
}
